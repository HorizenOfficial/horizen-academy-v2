{
    "name": "horizen-academy-v2",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "env:local": "dotenv -e ./env/.env.local",
        "env:dev": "dotenv -e ./env/.env.dev",
        "env:dev-horizen": "dotenv -e ./env/.env.dev_horizen",
        "env:prod": "dotenv -e ./env/.env.prod",
        "env:prod-horizen": "dotenv -e ./env/.env.prod_horizen",
        "start": "npm run env:local docusaurus start",
        "build": "npm run env:local docusaurus build",
        "build:dev": "npm run env:dev docusaurus build",
        "build:dev-horizen": "npm run env:dev-horizen docusaurus build",
        "build:prod": "npm run env:prod docusaurus build",
        "build:prod-horizen": "npm run env:prod-horizen docusaurus build",
        "serve": "npm run env:local docusaurus serve",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "dependencies": {
        "@docusaurus/core": "2.3.1",
        "@docusaurus/preset-classic": "2.3.1",
        "@mdx-js/react": "1.6.22",
        "@tanstack/react-query": "4.20.4",
        "axios": "1.2.1",
        "clsx": "1.2.1",
        "hast-util-is-element": "1.1.0",
        "prism-react-renderer": "1.3.5",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-hook-form": "7.41.2",
        "rehype-katex": "5.0.0",
        "remark-math": "3.0.1"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "2.3.1",
        "@tsconfig/docusaurus": "1.0.5",
        "@typescript-eslint/eslint-plugin": "5.47.0",
        "@typescript-eslint/parser": "5.47.0",
        "dotenv-cli": "7.0.0",
        "eslint": "8.30.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.31.11",
        "eslint-plugin-react-hooks": "4.6.0",
        "prettier": "2.8.1",
        "typescript": "4.7.4"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=16.14"
    }
}
